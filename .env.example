# API Keys for Google Trends Integration
# Get a SERP API key from https://serpapi.com/
SERP_API_KEY=your_serp_api_key_here

# LLM API Keys for Content Generation
# Get Groq API key from https://console.groq.com/ (preferred - fast and free tier)
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=mixtral-8x7b-32768

# OR use OpenRouter as fallback
# Get OpenRouter API key from https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=anthropic/claude-3-sonnet

# OpenAI API Key (if using OpenAI features)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (if using Claude Code)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenCode Configuration
OPENCODE_API_KEY=your_opencode_api_key_here
